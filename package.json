{
  "name": "smart-storage",
  "version": "0.0.1",
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "postinstall": "rimraf ./.husky && husky install && pnpm husky:pre-commit-hook && pnpm husky:commit-msg-hook",
    "husky:pre-commit-hook": "husky add .husky/pre-commit 'pnpm lint-staged'",
    "husky:commit-msg-hook": "husky add .husky/commit-msg 'pnpm commitlint'",
    "play:vue-demo": "pnpm -F smart-storage-vue-demo start",
    "play:react-demo": "pnpm -F smart-storage-react-demo start",
    "docs:start": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "pnpm -F \"@smart-storage/*\" test",
    "build": "pnpm -F \"@smart-storage/*\" build",
    "build:pages": "pnpm build && pnpm docs:build && node ./scripts/build-pages.js",
    "format": "pnpm -F \"@smart-storage/*\" format",
    "eslint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit ${1}",
    "clean-all": "pnpx rimraf \"./node_modules\" --impl=native && pnpm -F \"*\" clean",
    "force-update": "pnpx rimraf ./pnpm-lock.yaml && pnpm install && pnpm build && pnpm clean-all && pnpx rimraf ./pnpm-lock.yaml && pnpm install"
  },
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.3.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/jest": "29.2.5",
    "@vuepress/plugin-back-to-top": "1.9.8",
    "eslint": "8.22.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "fs-extra": "11.1.0",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "live-server": "1.2.2",
    "prettier": "2.7.1",
    "rimraf": "4.0.7",
    "rollup": "3.10.0",
    "shelljs": "0.8.5",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4",
    "vue": "2.7.14",
    "vuepress": "1.9.2"
  }
}
